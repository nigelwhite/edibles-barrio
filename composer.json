{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^2.2",
        "drupal/asset_injector": "^2.6",
        "drupal/backup_migrate": "^4.0",
        "drupal/block_styles": "^1.2",
        "drupal/blockgroup": "^1.1",
        "drupal/bootstrap_barrio": "^4.17",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/coffee": "^1.0@beta",
        "drupal/commerce": "^2.11",
        "drupal/commerce_reports": "^1.0@beta",
        "drupal/commerce_stock": "1.x-dev",
        "drupal/commerce_stripe": "^1.0@RC",
        "drupal/computed_field": "^2.0",
        "drupal/console": "^1.0.2",
        "drupal/core": "8.8.8",
        "drupal/crop": "^1.5",
        "drupal/ctools": "^3.0",
        "drupal/daterange_compact": "1.x-dev",
        "drupal/devel": "^2.1",
        "drupal/ds": "^3.1",
        "drupal/embed": "^1.0",
        "drupal/entity": "^1.0",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/examples": "1.x-dev",
        "drupal/field_formatter_class": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/honeypot": "^1.30",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/libraries": "^3.0@alpha",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/module_filter": "^3.1",
        "drupal/pathauto": "^1.6@beta",
        "drupal/profile": "^1.0",
        "drupal/responsive_slideshow": "^1.2",
        "drupal/rules": "^3.0@alpha",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/styles_api": "^1.0",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/views_bootstrap": "^4.1",
        "drupal/views_slideshow": "^4.6",
        "drupal/views_taxonomy_term_name_depth": "6.x-dev",
        "drush/drush": "^9.0.0",
        "twig/twig": "1.42.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/block_styles": {
                "Drupal sort error on config of block": "https://www.drupal.org/files/issues/2642010-7-fix-block-notice.patch"
            }
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
